
public class ccrud {

	public static void main(String[] args) {
		char [] _memory = {'5','a','4'};
	//first line
		for(int i=0;i<1;i++){
			System.out.print(" 0");	
		}
		for(int i=0;i<1;i++){
			System.out.print("|");	
		}
		for(int j=0 ;j< 20;j++){
			System.out.print(".");
		
	}
		for(int i=0;i<1;i++){
			System.out.println("|");	
		}
		//second line
		for(int i=0;i<1;i++){
			System.out.print("20");
		}
		for(int i=0;i<1;i++){
			System.out.print("|");	
		}
		for(int j=0 ;j< 20;j++){
			System.out.print(".");
		
	}
		for(int i=0;i<1;i++){
			System.out.println("|");	
		}
		//third line
		for(int i=0;i<1;i++){
			System.out.print("40");	
		}
		for(int i=0;i<1;i++){
			System.out.print("|");	
		}
		for(int j=0 ;j< 20;j++){
			System.out.print(".");
		
	}
		//fourth line
		for(int i=0;i<1;i++){
			System.out.println("|");	
		}
		for(int i=0;i<1;i++){
			System.out.print("60");
		}
		for(int i=0;i<1;i++){
			System.out.print("|");	
		}
		for(int j=0 ;j< 20;j++){
			System.out.print(".");
		
	}
		for(int i=0;i<1;i++){
			System.out.println("|");	
		}
		for(int i=0;i<1;i++){
			System.out.print("80");
		}
		for(int i=0;i<1;i++){
			System.out.print("|");	
		}
		for(int j=0 ;j< 20;j++){
			System.out.print(".");
		
	}
		for(int i=0;i<1;i++){
			System.out.println("|");	
		}
		System.out.print(_memory);
		
	}
}

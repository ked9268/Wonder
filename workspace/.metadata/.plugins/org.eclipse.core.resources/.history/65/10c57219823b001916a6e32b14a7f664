
public class ccrud {

	public static void main(String[] args) {
	
		for(int i=0;i<1;i++){
			System.out.print("0");	
		}
		for(int i=0;i<1;i++){
			System.out.print("|");	
		}
		for(int j=0 ;j< 20;j++){
			System.out.print(".");
		
	}
		for(int i=0;i<1;i++){
			System.out.println("|");	
		}
		for(int i=0;i<1;i++){
			//System.out.print("\n");
			System.out.print("1");
		}
	}
}

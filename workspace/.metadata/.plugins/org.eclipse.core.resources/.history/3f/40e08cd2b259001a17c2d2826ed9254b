	public class action_leaderboard_mm implements ActionListener {

		
		public void actionPerformed(ActionEvent arg0) {
			Leaderboard lb = new Leaderboard();
			JFrame frame = lb.getJFrame();
			lb.setJFrame(frame);
			frame.remove(getPanel());
			
		}

	}


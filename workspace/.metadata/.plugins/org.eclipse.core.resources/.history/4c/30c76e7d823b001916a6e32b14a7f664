
public class ccrud {

	public static void main(String[] args) {
	//first line
		for(int i=0;i<1;i++){
			System.out.print("0");	
		}
		for(int i=0;i<1;i++){
			System.out.print("|");	
		}
		for(int j=0 ;j< 20;j++){
			System.out.print(".");
		
	}
		for(int i=0;i<1;i++){
			System.out.println("|");	
		}
		//second line
		for(int i=0;i<1;i++){
			System.out.print("1");
		}
		for(int i=0;i<1;i++){
			System.out.print("|");	
		}
		for(int j=0 ;j< 20;j++){
			System.out.print(".");
		
	}
		for(int i=0;i<1;i++){
			System.out.println("|");	
		}
		//third line
		for(int i=0;i<1;i++){
			System.out.print("2");	
		}
		for(int i=0;i<1;i++){
			System.out.print("|");	
		}
		for(int j=0 ;j< 20;j++){
			System.out.print(".");
		
	}
		//fourth line
		for(int i=0;i<1;i++){
			System.out.println("|");	
		}
		for(int i=0;i<1;i++){
			System.out.print("3");
		}
		for(int i=0;i<1;i++){
			System.out.print("|");	
		}
		for(int j=0 ;j< 20;j++){
			System.out.print(".");
		
	}
		//fifth line
		for(int i=0;i<1;i++){
			System.out.println("|");	
		}
	}
}
